{
    "name": "Pro Tube",
    "description": "Pro Tube helps you know how much time you can save while watching Youtube",
    "version": "0.0.1",
    "icons": {"256": "ProTube.png"},
    "manifest_version": 2,
    "background":{
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.youtube.com/*"
    ]
}